<!-- open graph -->
<meta property="og:title" content="{{ site.open_graph.title }}" />
<meta property="og:description" content="{{ site.open_graph.description|apply_filters('meta_description')  }}" />
<meta property="og:type" content="{{ site.open_graph.type }}" />
<meta property="og:url" content="{{ site.open_graph.url }}" />
<meta property="og:site_name" content="{{ site.open_graph.site_name }}" />
<meta property="og:image" content="{{ site.open_graph.image }}" />