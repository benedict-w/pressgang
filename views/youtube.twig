<!-- youtube embed -->
{% extends 'embed.twig' %}

{% block embed %}
    <iframe id="youtube-player-{{ id|e('html_attr') }}" type="text/html" width="{{ width|e('html_attr')|default(640) }}" height="{{ height|e('html_attr')|default(640) }}"
            src="https://www.youtube.com/embed/{{ id|e('html_attr') }}?enablejsapi={{ enablejsapi }}&origin={{ origin }}&autoplay={{ autoplay }}&loop={{ loop }}"
            frameborder="0"></iframe>
{% endblock %}