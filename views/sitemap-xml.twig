<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for node in nodes %}
    <url>
        <loc>{{ node['loc']|e }}</loc>
        {% if node['lastmod'] %}
        <lastmod>{{ node['lastmod'] }}</lastmod>
        {% endif %}
        <changefreq>{{ node['changefreq'] }}</changefreq>
        <priority>{{ node['priority'] }}</priority>
    </url>
{% endfor %}
</urlset>