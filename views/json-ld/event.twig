{% extends 'json-ld/script.twig' %}

{% block schema %}
  {
  "@context": "http://schema.org",
  "@type": "event",
  "name": "{{ name|e('html_attr') }}",
  "description": "{{ description|e('html_attr') }}",
  "image": "{{ image|e('html_attr') }}",
  "url": "{{ url|e('html_attr') }}",
  "startDate": "{{ start_date|e('html_attr') }}",
  "endDate": "{{ end_date|e('html_attr') }}",
  "location": "{{ location|e('html_attr') }}"
  }
{% endblock %}
